{
  "name": "reactBoilerPlate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint \"**/*.{js,jsx}\"",
    "format": "npm run prettier -- --write",
    "prettier": "prettier \"**/*.{js,jsx,css,json}\"",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "pretest:e2e:run": "npm run prod",
    "test:e2e:run": "start-server-and-test start http://localhost:8080/ cy:run",
    "test:e2e:dev": "start-server-and-test start http://localhost:8080/ cy:open",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "validate": "npm run lint && npm run prettier -- --list-different && npm test && npm run test:coverage && npm run test:e2e:run",
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server",
    "dev": "npm run webpack -- --env.mode development",
    "prod": "npm run webpack -- --env.mode production",
    "prod:analyze": "npm run prod -- --env.presets analyze",
    "prod:compress": "npm run prod -- --env.presets compress",
    "start": "npm run webpack-dev-server -- --env.mode development --watch --open --hot"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test",
      "pre-push": "lint-staged && npm test && npm run test:coverage && npm run test:e2e:run"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pablovegau/reactBoilerplate.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pablovegau/reactBoilerplate/issues"
  },
  "homepage": "https://github.com/pablovegau/reactBoilerplate#readme",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@testing-library/cypress": "^4.0.1",
    "@testing-library/react": "^8.0.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-dynamic-import": "^2.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^2.0.0",
    "css-loader": "^2.1.1",
    "cypress": "^3.3.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.1",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "jest-axe": "^3.1.1",
    "jest-dom": "^3.4.0",
    "lint-staged": "^8.1.7",
    "mini-css-extract-plugin": "^0.7.0",
    "prettier": "^1.17.1",
    "react-test-renderer": "^16.8.6",
    "start-server-and-test": "^1.9.1",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.32.2",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
